#include <stdio.h>
#include <conio.h>


void main()
{
    int i;
    int j;
    int pre=0;
    int cur=0;
    int a;
    
    printf("ENTER NUMBER\n");
    scanf("%d",&i);
    printf("FIBONACCI SERIES:\n");
    
    for(j=0;j<i;j++)
    {
        printf("%d\n",cur);
        a=pre+cur;
        pre=cur;
        if(a==0)
        {
            a=1;
        }
        cur=a;
    }
    
}
